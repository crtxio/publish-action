{
  "name": "@crtxio/primitives",
  "version": "0.0.1",
  "author": "Josh Robinson <josh@bproto.io>",
  "license": "MIT",
  "packageManager": "yarn@3.2.1",
  "type": "module",
  "files": [
    "dist",
    "package.json"
  ],
  "typesVersions": {
    "*": {
      "*": [
        "dist",
        "dist/*"
      ]
    }
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./cid": {
      "import": "./dist/cid.mjs",
      "require": "./dist/cid.cjs"
    },
    "./bytes": {
      "import": "./dist/bytes.mjs",
      "require": "./dist/bytes.cjs"
    }
  },
  "scripts": {
    "test": "mocha",
    "lint": "eslint .",
    "build": "unbuild",
    "format": "prettier --write \"**/*.+(ts)\""
  },
  "unbuild": {
    "roodDir": ".",
    "entries": [
      "./src/index",
      "./src/cid",
      "./src/bytes"
    ],
    "rollup": {
      "emitCJS": true
    },
    "declaration": true,
    "dts": {
      "respectExternal": true
    }
  },
  "mocha": {
    "require": "jiti/register",
    "extensions": [
      "ts"
    ],
    "spec": [
      "test/**/*.test.*"
    ],
    "watch-files": [
      "src",
      "test"
    ]
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/eslint": "^8",
    "@types/mocha": "^9.1.1",
    "@types/prettier": "^2",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "chai": "^4.3.6",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "mocha": "^10.0.0",
    "prettier": "^2.6.2",
    "unbuild": "^0.7.4"
  },
  "dependencies": {
    "@crtxio/bases": "^0.0.1",
    "@crtxio/hashers": "^0.0.1",
    "@crtxio/utils": "^0.0.1",
    "@noble/bls12-381": "^1.2.0",
    "@noble/hashes": "^1.0.0",
    "@noble/secp256k1": "^1.5.5",
    "eip-712": "^1.0.0"
  }
}