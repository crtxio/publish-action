{
  "name": "@crtxio/hddata",
  "version": "0.0.2",
  "author": "Josh Robinson <josh@bproto.io>",
  "license": "MIT",
  "packageManager": "yarn@3.2.1",
  "type": "module",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "package.json"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "test": "mocha",
    "lint": "eslint .",
    "build": "unbuild",
    "format": "prettier --write \"**/*.+(ts)\""
  },
  "unbuild": {
    "entries": [
      "./src/index"
    ],
    "rollup": {
      "emitCJS": true
    },
    "declaration": true
  },
  "mocha": {
    "require": "jiti/register",
    "extensions": [
      "ts"
    ],
    "spec": [
      "test/**/*.test.*"
    ],
    "watch-files": [
      "src",
      "test"
    ]
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/eslint": "^8",
    "@types/mocha": "^9.1.1",
    "@types/prettier": "^2",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "chai": "^4.3.6",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "mocha": "^10.0.0",
    "prettier": "^2.6.2",
    "unbuild": "^0.7.4"
  },
  "dependencies": {
    "@crtxio/primitives": "^0.0.1",
    "@crtxio/utils": "^0.0.1",
    "@noble/secp256k1": "^1.7.1",
    "cborg": "^1.9.4",
    "multiformats": "^11.0.1"
  }
}